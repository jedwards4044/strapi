{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Pages",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Body": {
      "type": "dynamiczone",
      "components": [
        "module.article-module",
        "module.banner-module",
        "module.parallax-module",
        "module.contact-form",
        "module.card-module",
        "module.photogallery-module",
        "module.photogrid-module",
        "module.testimonials-module",
        "module.map-module"
      ]
    },
    "name": {
      "type": "string"
    },
    "slug": {
      "type": "string"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "pagedata.seo"
    },
    "homePage": {
      "type": "boolean",
      "default": false
    },
    "ai": {
      "type": "customField",
      "options": {
        "length": "250"
      },
      "customField": "plugin::ai-text-generation.text-ai"
    },
    "page_preview": {
      "type": "customField",
      "customField": "plugin::pagepreview.page-preview"
    }
  }
}
